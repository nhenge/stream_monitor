{
  "name": "Monitor",
  "version": "0.9",
  "description": "monitor your things",
  "permissions": [
    "alarms",
    "bookmarks",
    "tabs",
    "storage",
    "https://api.twitch.tv/"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
    "default_popup": "monitor.html",
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": [ "background.js", "lib.js" ]
  },
  "manifest_version": 2
}
